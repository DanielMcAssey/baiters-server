{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/server-.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "GLOKON.Baiters.Server"
    }
  },
  "Server": {
    "ListenOn": "0.0.0.0",
    "ListenOnSocket": null,
    "ListenOnNamedPipe": null,
    "UseHsts": false,
    "HttpPort": 8080,
    "HttpsPort": 8081,
    "MaxUploadSize": 100000000,
    "LetsEncrypt": {
      "UseStagingServer": false,
      "Domains": [],
      "EmailAddress": ""
    },
    "SSL": {
      "CertificatePath": null,
      "CertificatePassword": null
    }
  },
  "WebFishing": {
    "Admins": [
      76561197988394297
    ],
    "PluginsEnabled": false,
    "ServerName": "My Baiters Server",
    "MaxPlayers": 50,
    "JoinType": "Public",
    "JoinMessage": null,
    "Tags": [
      "talkative",
      "quiet",
      "grinding",
      "chill",
      "silly",
      "hardcore",
      "mature",
      "modded"
    ],
    "Modifiers": {
      "FishChance": 0.50,
      "RainChance": 0.25,
      "MeteorChance": 0.01,
      "VoidPortalChance": 0.005,
      "TicksPerSecond": 24,
      "MaxMetal": 7,
      "MaxNpcActors": 50,
      "MaxFish": 30
    }
  }
}
